});

$("#objfrmForm").bind("submit", function() {
 
 	if ($("#objNomec").val().length < 1 || $("#objEmailc").val().length < 1 || $("#objMensagemc").val().length < 1)  {
	    $(".required").show();
	    $.fancybox.resize();
	    return false;
	}

