		type		: "POST",
		contentType: 'application/x-www-form-urlencoded; charset=iso-8859-1;', 
		cache	: false,
		url		: "DESTINO",
		data		: $(this).serializeArray(),
		success: function(data) {
			$.fancybox(data);
		}
	});

	return false;
});
		
});

