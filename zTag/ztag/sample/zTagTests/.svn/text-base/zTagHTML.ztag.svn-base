<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html lang="pt:br">
  <head>
    <meta http-equiv="content-type" content="text/html; charset=iso-8859-1" />
    <title>zForm - ZTags</title>
    <link rel="Shortcut Icon" href="/favicon.ico" type="image/x-icon" />
    <style type="text/css" media="screen, projection">
      body {margin: 0px 0px 0px 10px;
            padding:0px 0px 0px 0px;
            font-family:Arial, Verdana, Helvetica, sans-serif;
            color:#444444; ;
            line-height:125%;}
      h3 {margin:10px 0px 0px 0px;}
      h4 {margin:10px 0px 0px 0px;}
      hr {margin:0px 0px 10px 0px;}
      dl {font-size:90%; line-height:125%; margin:0px;}
      dt {color:navy; font-size:90%; padding-left:0px; margin:3px 0px 0px 0px; font-weight:bold;}
      dd {margin:0px 0px 0px 10px;}      
    </style>
  </head>
  <body>
    <h3>zHTML- Gestão do HTML</h3>
    <hr />
    <br />
    <h4>HTML:</h4>
    <hr />

    <!-- Block tags -->    
    <zhtml:h1 value="tagValueH1" style="tagStle" />
    <zhtml:h2 value="tagValueH2" style="tagStle" />
    <zhtml:h3 value="tagValueH3" style="tagStle" />
    <zhtml:h4 value="tagValueH4" style="tagStle" />
    <zhtml:h5 value="tagValueH5" style="tagStle" />
    <zhtml:h6 value="tagValueH6" style="tagStle" />

    <zhtml:a uri="tagURI" value="tagValue" target="tagTarget" />
    <zhtml:link uri="tagURI" value="tagValue" target="tagTarget" />

    <zhtml:pre value="tagValuePre" style="tagStyle" />
    
    <zhtml:code value="tagValueCode" style="tagStyle" />

    <zhtml:title value="tagValueTitle" />
    <zhtml:code value="tagValueCode" />
    <zhtml:u value="tagValueU" />
    <zhtml:b value="tagValueB" />
    <zhtml:s value="tagValueS" />
    <zhtml:i value="tagValueI" />
    <zhtml:em value="tagValueEM" />
    <zhtml:cite value="tagValueCite" />
    <zhtml:sup value="tagValueSUP" />
    <zhtml:sub value="tagValueSUB" />
    <zhtml:center value="tagValueCenter" />
    <zhtml:kbd value="tagValueKBD" />
    <zhtml:ins value="tagValueINS" />
    <zhtml:dfn value="tagValueDFN" />
    <zhtml:del value="tagValueDEL" />
    <zhtml:address value="tagValueAddress" />

    <zhtml:commment value="tagValueComment" />

    <zhtml:p value="tagValue" />

    <zhtml:p value="tagValue">
    </zhtml:p>

    <zhtml:span value="tagValue" tagid="tagId" />

    <zhtml:span tagid="tagId">
    </zhtml:span>

    <zhtml:div value="tagValue" tagid="tagId" />

    <zhtml:div tagid="tagId">
    </zhtml:div>

    <!-- No Block -->    
    <zhtml:img src="tagSRC" value="tagValueIMG" />
    <zhtml:basefont face="tagFace" color="tagColor" size="tagValueBaseFont" />
    <zhtml:base href="tagBaseHRef" target="tagBaseTarget" />

  </body>
</html>
