      <zform:input type="text" name="fieldName" tagid="fieldId" value="fieldValue" />

      <br /><zform:inputtext name="textName" tagid="textId" value="textValue" />
      <br /><zform:inputhidden name="hiddenName" tagid="hiddenId" value="hiddenValue" />
      <br /><zform:inputpassword name="passwordName" tagid="passwordId" value="passwordValue" />
      <br /><zform:inputcheckbox name="checkboxName" tagid="checkboxId" value="checkboxValueOn" values="checkboxValueOn;checkboxValueOff" />
      <br /><zform:textarea name="passwordName" tagid="passwordId" value="passwordValue" />

