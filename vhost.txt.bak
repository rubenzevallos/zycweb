# ZYC - Lugar Medico
<VirtualHost *:80>
  ServerName lugarmedico1.zyc.com.br
  DocumentRoot "C:/My Dropbox/ZYCWeb/LugarMedico"
  ErrorLog "C:/Web/unidental.com.br/log/lugarmedico180_error.log"
  CustomLog "C:/Web/unidental.com.br/log/lugarmedico180_custom.log" combined

 <Directory "C:/My Dropbox/ZYCWeb/LugarMedico">
    Options Indexes FollowSymLinks
    AllowOverride All
    Order allow,deny
    Allow from all
  </Directory>
  Alias /db C:/My Dropbox/ZYCWeb/zTag/db

 <Directory "C:/My Dropbox/ZYCWeb/zTag/db">
    Options Indexes FollowSymLinks
    AllowOverride All
    Order allow,deny
    Allow from all
  </Directory>

  Alias /ztag C:/My Dropbox/ZYCWeb/zTag/ztag

 <Directory "C:/My Dropbox/ZYCWeb/zTag/ztag">
    Options Indexes FollowSymLinks
    AllowOverride All
    Order allow,deny
    Allow from all
  </Directory>
</VirtualHost>

