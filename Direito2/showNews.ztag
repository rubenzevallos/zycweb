					<br /> 
					<small class="commentmetadata"><zhtml:a title="$lcRow[dt_inclusao]" rel="nofollow" href="#comentario-$lcRow[cd_comentario]" value="$lcRow[dt_inclusao_day] de $lcRow[nm_mes] de $lcRow[dt_inclusao_year]" /></small>
					<div id="HOTWordsTxt"><zhtml:p value="$lcRow[ds_comentario]" /></div>
				</li>
			</zdb:fetchuntil>
		</ol>	 	
