		<zdb:query use="D2Conn" update="listComments">
			SELECT SQL_CACHE cd_comentario 
			, nm_usuario
			, ds_cidade
			, ds_estado
			, ds_atividade
			, ds_url
			, ds_comentario
			, LOWER(nm_mes) nm_mes
			, DAY(c.dt_inclusao) dt_inclusao_day
			, YEAR(c.dt_inclusao) dt_inclusao_year
			, DATE_FORMAT(c.dt_inclusao, '%d/%m/%Y') dt_inclusao
			FROM tb_comentario c
			LEFT JOIN tb_mes ON cd_mes = MONTH(c.dt_inclusao)
			LEFT JOIN tb_usuario u ON c.cd_usuario = u.cd_usuario 
			WHERE c.fl_ativo = 1 AND c.cd_noticia = $snRow[cd_noticia]
			ORDER BY cd_comentario DESC
		</zdb:query>
		
		<ol class="commentlist">
			<zdb:fetchuntil use="listComments" var="lcRow">
				<li id="comentario-<zvar:show use="$lcRow[cd_comentario]" />" class="alt">
