		 <zdb:query use="D2Conn" id="listSource">
			SELECT SQL_CACHE DISTINCT dt_referencia_year dt_year
			, dt_referencia_month dt_month
			, dt_referencia_day dt_day
			, nm_fonte
			, sg_fonte
			, REPLACE(sg_fonte, ' ', '') sg_fonte_c
			, LOWER(REPLACE(sg_fonte, ' ', '')) sg_fonte_cl
			, nm_mes
			, LOWER(sg_mes3) sg_mes3
			, COUNT(cd_noticia) qt_source
			FROM tb_noticia n
			LEFT JOIN tb_mes ON cd_mes = dt_referencia_month
			LEFT JOIN tb_fonte f ON n.cd_fonte = f.cd_fonte
			WHERE n.fl_ativo = 1 AND dt_referencia_year = $gYear AND sg_mes3 = '$gMonth' AND dt_referencia_day = $gDay
			GROUP BY dt_referencia_year, dt_referencia_month,  dt_referencia_day
			ORDER BY dt_referencia_year DESC, dt_referencia_month DESC,  dt_referencia_day DESC
		 </zdb:query>

		<!-- http://www.direito2.com.br/stf/2011/abr/19/  -->
		<ul>
		<zdb:fetchuntil use="listSource" var="lsRow">
		   <li><zhtml:a href="/$lsRow[sg_fonte_cl]/$lsRow[dt_year]/$lsRow[sg_mes3]/$lsRow[dt_day]" value="$lsRow[sg_fonte] - $lsRow[nm_fonte]" title="$lsRow[nm_fonte] - Notícias ($lsRow[qt_source])"/></li>
		 </zdb:fetchuntil>
  </ul>
<zdb:close use="D2Conn" />
